    "babel-core": "^5.8.20",
    "babel-runtime": "^5.8.20",
    "bootstrap": "^3.3.5",
    "highlight.js": "^8.8.0",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^0.11.3",
    "babel": "^5.8.21",
